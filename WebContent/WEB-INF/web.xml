<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">
    <display-name>facebookfriends</display-name>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <description>Servlet that Fetch Facebook Friends List</description>
        <display-name>FriendsListServlet</display-name>
        <servlet-name>FriendsListServlet</servlet-name>
        <servlet-class>com.bitspedia.servlets.FriendsListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FriendsListServlet</servlet-name>
        <url-pattern>/FriendsListServlet</url-pattern>
    </servlet-mapping>
</web-app>
